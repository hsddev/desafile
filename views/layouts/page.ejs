    <script src="/socket.io/socket.io.js"></script>
    <script>
      window.notifications = <%- JSON.stringify(notifications) %>;
      window.initNotifications = function() {
        var notifications = window.notifications;
        var notificationsCount = notifications.filter(function(noti) { return !noti.seen}).length;
        notifications = notifications.sort(function(a, b){return new Date(a.createdAt) - new Date(b.createdAt);});

        if(notificationsCount > 0) {
          $('#notification-count').html(notificationsCount);
          $('#notification-count').removeAttr("style")
        } else {
          $('#notification-count').css('display','none');
        }
        if(notifications.length > 0 ) {
          notifications.forEach(function(notification,i) {
            var time = moment(notification.createdAt).fromNow();
            var htmlNoti = '<a href="'+ notification.link +'" class="kt-notification__item">\
                      <div class="kt-notification__item-icon">\
                        <i class="flaticon-file"></i>\
                      </div>\
                      <div class="kt-notification__item-details">\
                        <div class="kt-notification__item-title">\
                          '+ (notification.seen ?  notification.description : '<strong>' + notification.description +'</strong>') +
                        '</div>\
                        <div class="kt-notification__item-time">' + time + '</div>\
                      </div>\
                    </a>';
                    if(i === 0) {
                      $('#notifications').html(htmlNoti);
                    } else {
                      $('#notifications').prepend(htmlNoti);
                    }
          })     
        } else {
          var noNotifs = '<div class="kt-head" style="height: 100%;"><div class="kt-heading kt-heading--thin">No notifications yet.</div></div>';
          $('#notifications').html(noNotifs);
        }
      }

      jQuery(document).ready(function() {
          window.initNotifications();
      });
    </script>
    <script>
      var socket = io.connect(window.location.protocol + '//' + window.location.host, {transports: ['polling']});
      socket.on('new notification', function (data) {
        if(Array.isArray(data.notifications) && data.new) {
          // Add to Notification
          if(window.notifications && Array.isArray(window.notifications)) {
            window.notifications = data.notifications;
            window.initNotifications();
          }

          // Play Sound
          var promise = document.querySelector('audio').play();
          if (promise !== undefined) {
              promise.then(_ => {
                  // Autoplay started!
              }).catch(error => {
                  // Autoplay was prevented.
                  // Show a "Play" button so that user can start playback.
              });
          }

          // Show toastr
          toastr.options.onclick = function() { window.open(window.location.origin + data.new.link, '_blank');}
          toastr.success(data.new.description,'<div style="color: #fff;">'+ data.new.title +'</div>');
          // send Notificaition
          if (window.Notification && Notification.permission !== "granted") {
            Notification.requestPermission(function (status) {
              if (Notification.permission !== status) {
                Notification.permission = status;
              }
            });
          }
          if (window.Notification && Notification.permission === "granted") {
            var img = '/assets/media/icons/upload.png';
            var notification = new Notification(data.new.title, {body: data.new.description, icon: img });
            notification.onclick = function(event) {
              event.preventDefault(); // prevent the browser from focusing the Notification's tab
              window.open(window.location.origin + data.new.link, '_blank');
            }
            setTimeout(notification.close.bind(notification), 10000);
          } else {
            alert(data.new.description);
          }
        }
  
      });
    </script>
